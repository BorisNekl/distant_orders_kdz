<Window x:Name="ProductWindow" x:Class="distant_orders_kdz.WindowProducts"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:distant_orders_kdz"
        mc:Ignorable="d"
        Title="WindowProducts" Height="514" Width="1006.5">
    <ScrollViewer HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" Grid.ColumnSpan="2" Grid.RowSpan="2" Margin="10,10,0,0">
        <Grid x:Name="ProductGrid" Margin="0,0,-8,-21">
            <Frame x:Name="ProductFrame" NavigationUIVisibility = "Hidden"/>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>

        </Grid>
    </ScrollViewer>
</Window>
